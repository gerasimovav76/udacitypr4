{
	"name": "df_EmpTitleMDtoDB",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_dl_TitleMaster",
						"type": "DatasetReference"
					},
					"name": "source1",
					"description": "Import data from ds_dl_TitleMD"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dl_TitleMaster",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"description": "Export data to ds_sqldb_NYCTitleMD"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> sink1"
			]
		}
	}
}